<%
if (diseases.length) {

	diseases.forEach(function(disease) {
		var str = disease.value;
%>

		<div class="disease-serach-list_item" onclick="window.edit_servey.loadServeyByDisease('<%=disease.key%>');">
			<span><%=str%></span>
		</div>

<%
	});

} else {
%>

	<div class="disease-serach-list_item">
		<span><%=_T("No diseases found")%></span>
	</div>

<% } %>
